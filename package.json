{
  "name": "babel-node-base",
  "version": "0.1.0",
  "description": "Base project for eslint and babel for modern javascript and node.js",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "eslint:fix": "eslint -c .eslintrc.json --fix src",
    "eslint:check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
    "production": "node dist",
    "start": "node -r babel-register src",
    "start:watch": "nodemon -r babel-register src"
  },
  "keywords": [
    "base",
    "javascript",
    "node.js"
  ],
  "author": "Jake Klassen",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.4.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.1.2",
    "nodemon": "^1.11.0",
    "prettier": "^1.5.3"
  },
  "dependencies": {}
}
