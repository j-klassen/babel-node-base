{
  "name": "babel-node-base",
  "version": "0.1.0",
  "description": "Base project for eslint and babel for modern javascript and node.js",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "dev": "babel-watch src",
    "start": "node dist",
    "test": "ava --tap | faucet"
  },
  "keywords": [
    "base",
    "javascript",
    "node.js"
  ],
  "author": "Jake Klassen",
  "license": "MIT",
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-watch": "^2.0.6",
    "eslint": "^3.19.0",
    "faucet": "^0.0.1",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "express": "^4.15.3"
  }
}
